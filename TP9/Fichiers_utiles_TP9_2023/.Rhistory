data(iris)
tirage<-sample(1:dim(iris)[1], 120, replace = FALSE)
#contrôle du tirage aléatoire
table(iris$Species[tirage])
#les données de l'échantillon
ech<-iris[ row.names(iris) %in% tirage,]
#les données test pour la validation
test<-iris[-tirage,]
#l'analyse discriminante pas à pas (bibliothèque MASS et klaR) library(MASS)
library(klaR)
install.packages("klaR")
library(klaR)
disc<-stepclass(Species~., data =ech,
method = "lda",improvement = 0.001)
#Graphique d'évolution des performances de classification :
plot(disc, main="Évolution des perf. de classification")
#description graphique des groupes. Renommer les groupes :
don<-ech
don$Species<-as.character(don$Species)
for (i in 1:dim(don)[1]) {
if (don$Species[i]=="virginica") don$Species[i]<-"i"
if (don$Species[i]=="versicolor") don$Species[i]<-"e"
if (don$Species[i]=="setosa") don$Species[i]<-"s" }
don$Species<-as.factor(don$Species)
#L'analyse discriminante - une description graphique des groupes :
partimat(Species~., data = don)
#Analyse discriminante - on retient 4 variables explicatives
library(mda)
discrim<-fda(Species~., data =ech)
install.packages("mda")
library(mda)
discrim<-fda(Species~., data =ech)
#classement des valeurs "test"
pred<-predict(discrim,test)
#calcul du nombre de mal classés parmi les valeurs "test"
confusion(pred,test$Species)
#L'analyse discriminante linéaire - le graphique :
plot(discrim)
abline(v=0,h=0)
#les coefficients linéaires
coef(discrim)
#les moyennes par classe dans l'espace discriminant
discrim$means
